{
  "name": "Skipper",
  "description": "Skip Movie/Series video scenes for popular streaming services like Netflix!",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "html/index.html"
  },
  "content_scripts": [{
    "matches": ["*://*.youtube.com/watch?v=*"],
    "js": ["js/dom.js"]
  }],
  "permissions" : [
    "activeTab"
  ]
}